{
  "codeLens.enable": true,
  "diagnostic-languageserver.filetypes": {
    "elixir": [
      "mix_credo",
      "mix_credo_compile"
    ],
    "eelixir": [
      "mix_credo",
      "mix_credo_compile"
    ]
  },
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": [
        "go.mod",
        ".vim/",
        ".git/",
        ".hg/"
      ],
      "filetypes": [
        "go"
      ]
    }
  }
}
